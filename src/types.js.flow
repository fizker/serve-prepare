// @flow strict

import type { Headers, Alias } from "@fizker/serve"

export type FileOverride = {
	path: string,
	mime: ?string,
	statusCode: ?number,
	headers: Headers,
}

export type SetupRequest = {
	aliases: $ReadOnlyArray<Alias>,
	globalHeaders: Headers,
	files: $ReadOnlyArray<FileOverride>,
	catchAllFile: ?FileOverride
}
