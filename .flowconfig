[ignore]
.*/node_modules/\.cache/.*
.*/\.git/.*

[include]

[libs]
flow-typed

[options]
exact_by_default=true

[lints]
# (https://flow.org/en/docs/linting/)
all=error

[strict]
sketchy-null
sketchy-number
untyped-type-import
untyped-import
unclear-type
unsafe-getters-setters
unnecessary-optional-chain
unnecessary-invariant
deprecated-utility
non-array-spread
nonstrict-import
